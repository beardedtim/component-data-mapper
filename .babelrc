{
  "presets": [
    ["env", {
        "targets": {
          "node": 6,
          "browsers": [
            "> 4%",
            "last 2 versions",
            "safari >= 7"
          ]
        },
        "loose": true,
        "useBuiltIns": true,
    }],
    "react",
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-flow-strip-types"
  ]
}
